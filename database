create database vietnameseculture;
use vietnameseculture;
create table baiviet(
	idPost int not null primary key auto_increment;
	titleContent varchar(100),
	content varchar(255),
	author varchar(50)
);

create table userss(
	Idcode int not null primary key auto_increment;
	UserName varchar(100),
	Passwords varchar(100),
	Fullname varchar(200),
	Age int,
	Address varchar(200),
	Phone varchar(15),
	Email varchar(50)
);
